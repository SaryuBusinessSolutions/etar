---
{
"title":"<%- title %>",
"category": <%-  typeof category == 'undefined' ? false : true %>,
"featured": <%-  typeof featured == 'undefined' ? false : true %>,
"name":"<%- name.toUpperCase() %>",
"type":"<%- type %>",
"img":"<%- img %>"
}
---

<div data-scroll-section class="maxhfix h-screen flex justify-center items-center">
  <%%- include('mini/img',{
    alt : "product",
    src : "../img/products/small/<%- img %>",
    md : "../img/products/large/<%- img %>",
    classes : "w-full md:w-[40%] z-[1]",
  }) %%>
  <p data-scroll data-scroll-speed="1"
    class="text-8xl md:text-[20rem] absolute text-white stroke text-clip overflow-hidden text-center"><%- name.toUpperCase() %></p>
</div>