<picture  class="<%- classes %>">
  <% if (typeof sm != 'undefined') { %>
  <source media="(min-width:640px)" :srcset="'<%- sm %>'+list.item.img">
  <% } if (typeof md != 'undefined') { %>
  <source media="(min-width:768px)" :srcset="'<%- md %>'+list.item.img">
  <% } if (typeof lg != 'undefined') { %>
  <source media="(min-width:1024px)" :srcset="'<%- lg %>'+list.item.img">
  <% } if (typeof xl != 'undefined') { %>
  <source media="(min-width:1280px)" :srcset="'<%- xl %>'+list.item.img">
  <% } %>
  <img :src="'<%- src %>'+list.item.img" alt="<%- alt %> <%- typeof attr != 'undefined' ? attr : "" %>">
</picture>